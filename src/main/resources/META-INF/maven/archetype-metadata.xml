<modules>

    <module id="${groupId}-parent" dir="biwan-parent" name="${groupId}-parent" >
        <fileSets>
            <fileSet filtered="true" packaged="true" encoding="UTF-8">
                <directory>src/main/java</directory>
                <includes>
                    <include>**/*.java</include>
                </includes>
            </fileSet>
        </fileSets>
    </module>

    <module id="${groupId}-biz" dir="biwan-biz" name="${groupId}-biz" >
